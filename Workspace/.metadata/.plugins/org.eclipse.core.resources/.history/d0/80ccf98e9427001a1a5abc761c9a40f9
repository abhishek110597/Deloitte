package Inheritance;

class Animal{
	 final void sleep() {
		System.out.println("Sleep");
	}
	
	 void eat() {
		 System.out.println("Animal eats");
	 }
}

class Dog extends Animal{
	void eat() {
		System.out.println("Dog eats");
	}
}

public class Inheritance {
public static void main(String[] args) {
	Dog d=new Dog();
	d.sleep();
	d.eat();
}
}
